{
  "name": "@kapmug/feathers-nano",
  "version": "2.0.0",
  "description": "A couchdb service for feathers",
  "main": "lib/",
  "scripts": {
    "watch": "babel --watch -d lib/ src/",
    "test": "echo \"Error: no test specified\" && exit 1",
    "compile": "rimraf lib/ && babel -d lib/ src/",
    "build": "npm run compile",
    "start": "node test/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kapmug/feathers-nano.git"
  },
  "keywords": [
    "feathers",
    "couchdb",
    "nodejs"
  ],
  "author": "Andre Bazaglia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bazaglia/feathers-nano/issues"
  },
  "homepage": "https://github.com/bazaglia/feathers-nano#readme",
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "feathers-errors": "^2.8.2",
    "uberproto": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "body-parser": "^1.17.2",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "feathers": "^2.1.4",
    "feathers-hooks": "^2.0.2",
    "feathers-rest": "^1.8.0",
    "feathers-service-tests": "^0.10.2",
    "nano": "^6.4.0",
    "rimraf": "^2.6.1"
  }
}
